{
  "name": "room-navigator",
  "version": "1.0.0",
  "description": "room-navigator project",
  "main": "dist/src/index.js",
  "jest": {
    "globals": {
      "tsConfigFile": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.(ts)"
    ],
    "testEnvironment": "node"
  },
  "engines": {
    "node": "12.x.x",
    "npm": "6.x.x"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "start": "npm run serve",
    "serve": "node dist/src/index.js",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "watch-node": "nodemon dist/src/index.js",
    "watch-ts": "tsc -w"
  },
  "author": "gianluca moretti",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.13",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "express": "^4.16.4",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "supertest": "^3.4.2",
    "ts-jest": "^27.0.7",
    "typemoq": "^2.1.0",
    "typescript-logging": "^0.6.3"
  }
}
